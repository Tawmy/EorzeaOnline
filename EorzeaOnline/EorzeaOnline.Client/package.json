{
  "devDependencies": {
    "tailwindcss": "^3.4.17",
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "css:build": "npx tailwindcss -i ./Styles/source.css -o ./wwwroot/css/tailwind.css --minify",
    "css:watch": "cross-env NODE_ENV=development ./node_modules/tailwindcss/lib/cli.js -i ./Styles/source.css -o ./wwwroot/css/tailwind.css --watch"
  }
}
